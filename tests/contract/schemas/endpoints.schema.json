{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "additionalProperties": false,
    "required": [
        "generatedAt",
        "mode",
        "auth",
        "rest",
        "ftp"
    ],
    "properties": {
        "generatedAt": {
            "type": "string",
            "format": "date-time"
        },
        "mode": {
            "type": "string",
            "enum": [
                "SAFE",
                "STRESS"
            ]
        },
        "auth": {
            "type": "string",
            "enum": [
                "ON",
                "OFF"
            ]
        },
        "rest": {
            "type": "array",
            "items": {
                "type": "object",
                "additionalProperties": false,
                "required": [
                    "id",
                    "method",
                    "path",
                    "group",
                    "safe"
                ],
                "properties": {
                    "id": {
                        "type": "string"
                    },
                    "method": {
                        "type": "string"
                    },
                    "path": {
                        "type": "string"
                    },
                    "group": {
                        "type": "string"
                    },
                    "safe": {
                        "type": "boolean"
                    },
                    "notes": {
                        "type": "string"
                    }
                }
            }
        },
        "ftp": {
            "type": "array",
            "items": {
                "type": "object",
                "additionalProperties": false,
                "required": [
                    "command",
                    "safe"
                ],
                "properties": {
                    "command": {
                        "type": "string"
                    },
                    "safe": {
                        "type": "boolean"
                    },
                    "notes": {
                        "type": "string"
                    }
                }
            }
        }
    }
}
